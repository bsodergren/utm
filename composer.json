{
    "name": "bsodergren/utm",
    "description": "Private Packages for my code.",
    "type": "library",
    "license": "GPL-2.0-only",
    "require": {
        "php": ">=8.0",
        "symfony/monolog-bundle": "^3.8",
        "symfony/console": "^6.3",
        "symfony/stopwatch": "^6.2",
        "camoo/config": "^1.0",
        "nesbot/carbon": "^2.71",
        "nette/utils": "^4.0@dev",
        "nette/robot-loader": "^4.0",
        "symfony/yaml": "^7.0",
        "salibhdr/php-dump-logger": "^2.1"
        },
        "require-dev": {
            "symfony/var-dumper": "^6.4|^7.0"
        },
    "authors": [
        {
            "name": "Bjorn Sodergren",
            "role": "Original Author"
        }
    ],
	"autoload": {
        "files": [ "src/Resources/dump.php","src/Resources/Options.php" ],

        "psr-4": {

            "UTM\\": "src/"
        },
		"classmap": ["src/"]
	},
   
    "minimum-stability": "dev",
    "prefer-stable": true
